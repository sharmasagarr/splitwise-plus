{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["es2023"],
    "target": "es2023",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "types": ["node"],
    "noEmit": false,
    "skipLibCheck": true,
    "rootDirs": ["src", "generated"] 
  },
  "include": ["src/**/*", "generated/**/*"]
}